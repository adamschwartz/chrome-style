{
  "name": "Style",
  "version": "2.0",
  "description": "Style the internet how you want.",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "css": [
      "style.css"
    ],
    "js": [
      "style.js"
    ],
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "run_at": "document_start"
  }]
}
